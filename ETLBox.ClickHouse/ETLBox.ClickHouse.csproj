<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <TargetFrameworks>net9.0;net8.0;net6.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>ETLBox.ClickHouse</Title>
        <Authors>choby</Authors>
        <PackageProjectUrl>https://github.com/choby/ETLBox.ClickHouse</PackageProjectUrl>
        <RepositoryUrl>https://github.com/choby/ETLBox.ClickHouse</RepositoryUrl>
        <Version>1.0.1</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="ETLBox" Version="3.7.0" />
      <PackageReference Include="JetBrains.Annotations" Version="2024.3.0" />
      <PackageReference Include="Masuit.Tools.Core" Version="2025.4.2" />
      <PackageReference Include="Octonica.ClickHouseClient" Version="3.1.3" />
    </ItemGroup>

</Project>
